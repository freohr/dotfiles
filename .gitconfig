[core]
	editor = vim
[user]
	name = Stephen FAURE
[push]
	default = simple
[rerere]
	enabled = true
[alias]
	unstage = restore --staged
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[pull]
	rebase = true
[rebase]
	autoStash = true
